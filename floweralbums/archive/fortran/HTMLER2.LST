             $          char(mod(5*j,10)+48)
     87             write(4,*) '<a class="bj2" href="./alb' // an //
             $                 pn2 // '.htm"> '
             $                 // pn2 // ' </a>'
     88  21     continue
     89         write(4,*) '</td>'
     90         write(4,*) '<td class="right">'
     91         if (i.eq.npg) then
     92           write(4,*) '<a class="bj" href="./index.html">',
             $               ' <h1> Cover </h1> </a>'
     93         else
     94           pn = char((i+1-mod(i+1,10))/10+48) //
             $         char(mod(i+1,10)+48)
     95           write(4,*) '<a class="bj" href="./alb' // an //
             $               pn // '.htm">',
