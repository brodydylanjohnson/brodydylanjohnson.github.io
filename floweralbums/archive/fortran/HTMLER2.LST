    139           write(4,*) 'onclick="tog()" >'
    140         else
    141           write(4,*) '<div id="bee" >'
    142         endif
    143         write(4,*) cap2
    144         write(4,*) '</div>'
    145         write(4,*) '</body>'
    146         write(4,*) '</html>'
    147         close(4)
    148  51   continue
    149       close(5)
        
    150       end


