     67         else
     68           write(4,*) '<div id="beej" >'
     69         endif
     70         write(4,*) cap2
     71         write(4,*) '</div>'
     72         write(4,*) '</td></tr>'
     73         write(4,*) '</table>'     74         write(4,*) '<br>'
     75         write(4,*) '<table class="nav">'
     76         write(4,*) '<tr><td class="left">'
     77         if (i.eq.1) then
