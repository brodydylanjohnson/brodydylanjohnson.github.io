    100         write(4,*) '</td></tr>'
    101         write(4,*) '</table>'
    102         write(4,*) '</body>'
    103         write(4,*) '</html>'
    104         close(4)
        
        C  WRITE FIRST PHOTO SOLO PAGE
    105         fname=pname1 // '.htm'
    106         open(4,file=fname,status='new')
    107         write(4,*) '<html>'
    108         write(4,*) '<head>'
