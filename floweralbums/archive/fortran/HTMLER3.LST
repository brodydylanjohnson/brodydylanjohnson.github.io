    116         write(4,*) '<br>'
    117         if (test1 .eq. 1) then
    118           write(4,*) '<div id="bee" onMouseOver="back(''' // pname1 //
             $               ''')" onMouseOut="front(''' // pname1 //
             $               ''')" '
    119           write(4,*) 'onclick="tog()"> '
    120         else
    121           write(4,*) '<div id="bee" >'
    122         endif
    123         write(4,*) cap1
    124         write(4,*) '</div>'
    125         write(4,*) '</body>'
    126         write(4,*) '</html>'
    127         close(4)
        
