    134         write(4,*) '<link rel="stylesheet" type="text/css"',
             $             'href="print.css" media="print" />'
    135         write(4,*) '<script src="hotchocolate.js"> </script>'
    136         write(4,*) '</head>'
    137         write(4,*) '<body>'
    138         write(4,*) '<img class="solo" src="./' // pname2 // '.jpg"',
