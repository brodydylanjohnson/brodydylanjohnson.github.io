    139         write(4,*) '<br>'
    140         if (test2 .eq. 1) then
    141           write(4,*) '<div id="bee" onMouseOver="back(''' // pname2 //
             $               ''')" onMouseOut="front(''' // pname2 //
             $               ''')" '
    142           write(4,*) 'onclick="tog()" >'
    143         else
    144           write(4,*) '<div id="bee" >'
    145         endif
    146         write(4,*) cap2
    147         write(4,*) '</div>'
    148         write(4,*) '</body>'
    149         write(4,*) '</html>'
    150         close(4)
    151  51   continue
    152       close(5)
        
    153       end

